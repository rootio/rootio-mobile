<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/synchronization_log_llt"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:padding="@dimen/activity_horizontal_margin" >

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:src="@drawable/synchronize" />

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1" >

        <TableRow android:layout_width="match_parent" android:layout_height="match_parent" >

            <TextView
                android:id="@+id/synchronization_log_date_tv"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/Smaller"
                android:layout_span="4"
                android:layout_weight="100"
                android:text="2013-03-04 12:34:43"
                android:textSize="@dimen/ListingText"
                android:textStyle="bold" />

        </TableRow>
        <TableRow android:layout_width="match_parent" android:layout_height="match_parent">

            <TextView
                
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginLeft="@dimen/Smaller"
                android:layout_weight="10"
                android:text="@string/SynchronizationLogHttpLabel" >

</TextView>

            <TextView
                android:id="@+id/synchronization_log_http_tv"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="20"
                android:text="as" >

</TextView>

<TextView
    
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:layout_weight="10"
    android:text="@string/SynchronizationLogChangesLabel" >

</TextView>

<TextView
    android:id="@+id/synchronization_log_changes_tv"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="@dimen/Smaller"
    android:layout_weight="20"
    android:text="ad" >

</TextView>
        </TableRow>
    </TableLayout>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:layout_margin="2dp"
        android:adjustViewBounds="true"
        android:scaleType="fitXY"
        android:src="@drawable/chevron" />

</LinearLayout>